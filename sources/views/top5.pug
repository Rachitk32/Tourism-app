html
  head
    link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css', integrity='sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk', crossorigin='anonymous')
    |         
    script(src='https://code.jquery.com/jquery-3.5.1.slim.min.js', integrity='sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj', crossorigin='anonymous')
    |         
    script(src='https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js', integrity='sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo', crossorigin='anonymous')
    |         
    script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js', integrity='sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI', crossorigin='anonymous')
  |     
-var title="#";
-var inx=1;
  body(style="background-color:#5CDB95")
    .container-fluid
       h2(class="text-center") Top 5 Places To Visit In #{locname}
       table.table.table-dark.table-hover.table-responsive-sm.table-borderless
        thead   
            tr(class="row")
                    th(scope="row" class="col-sm-1 text-center") #{title}
                    th(scope="row" class="col-sm-2 text-center") Name
                    th(scope="row" class="col-sm-7 text-center") Description
                    th(scope="row" class="col-sm-2 text-center") Rating
        tbody
            each item in items
                tr(class="row")
                    th(scope="row" class="col-sm-1 text-center") #{inx++}
                    th(scope="row" class="col-sm-2 text-center")
                        a(style="color:white" target="_blank"  href=item.web_url ) #{item.name}
                    th(scope="row" class="col-sm-7 text-justify") #{item.description}
                    th(scope="row" class="col-sm-2 text-center") #{item.rating}
                    
                


   
      
   

